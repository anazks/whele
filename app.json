{
  "expo": {
    "name": "Wheel aligment Info",
    "slug": "wheel",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.jpg",

    "resizeMode": "contain", 
    "scheme": "wheel",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/LogoNew.png",
        "backgroundColor": "#ffffff",
        "resizeMode": "contain"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anazk.wheel"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/lgo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bf007560-75b2-45f0-9fb6-08b63c7192b9"
      }
    },
    "owner": "anazk08"
  }
}
